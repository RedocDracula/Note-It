{
  "manifest_version": 2,
  "name": "Sticky Notes",
  "version": "1.0",

  "icons":{
  	"48":"icons/icon_48.png"
  },

  "browser_action":{
  	"default_popup":"popup/note.html",
  	"default_icon":{
  		"48":"icons/icon_48.png"
  	}
  },

  "permissions":[
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/contents.js"]
    }
  ]



}